		<script type="text/javascript">
			mui.plusReady(function(){
				var self =plus.webview.currentWebview();
				initShareContent(self.i,self.gid);
				
				
				
			})
			
			function initShareContent(i,gid){
				var zoo =app.serverApi(app.apiConstant.getShareNews);
				var sUrl =zoo+encodeURI("?i="+i+"&gid="+gid);
					
				mui.init({
				subpages: [
					{
						url: sUrl,
						id: 'out_newscontent',
					}
				  ]
				})
