<!doctype html>
<html>

	<head>
		<meta charset="UTF-8">
		<title></title>
		<meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1,maximum-scale=1,user-scalable=no" />
		<link href="css/mui.min.css" rel="stylesheet" />
		<link rel="stylesheet" type="text/css" href="css/style.css"/>
		<style type="text/css">
			.mui-table-view:before,.mui-table-view:after{
				display: none;
			}
			.mui-table-view-cell:last-child:after, .mui-table-view-cell:last-child:before{
				height: 1px;
			}
			.mui-table-view-cell:after{
				left: 0;
			}
			.mui-table-view-cell{
				padding: 11px 10px;
			}
		</style>
	</head>

	<body>
		<header class="mui-bar mui-bar-nav">
		    <a class="mui-action-back mui-icon mui-icon-left-nav mui-pull-left"></a>
		    <h1 class="mui-title">Candy Club정보</h1>
		</header>
		<div class="mui-content">
		    <ul class="mui-table-view">
		            <li class="mui-table-view-cell" v-for="item in abouts">
		                <a class="mui-navigate-right" @tap ="onDetail(item)" >
		                    {{item.title}}
		                </a>
		            </li>
		        </ul>
		</div>
		<script src="js/mui.min.js"></script>
		<script src="js/app.js" type="text/javascript" charset="utf-8"></script>
		<script src="js/vue.min.js" type="text/javascript"></script>
		<script type="text/javascript">
			var abouts =[
				{count:"1",title:"Candy Club이란？"}
				
				
				
				
				
				
				
			]
			
			
			
			mui.init()
			var webview_detail = null; //详情页webview
			mui.plusReady(function(){
				webview_detail = mui.preload({
					url: 'aboutusdatail.html',
					id: 'aboutusdatail',
				});
				
			})
			
			
			var aboutData = new Vue({
				el:'.mui-content',
				data:{
					abouts:abouts,
				},
				methods: {
					onDetail(item){
						webview_detail.show("slide-in-right", 300);
						var bt = item.title; 
						var count =item.count;
						mui.fire(webview_detail, 'opendatail', {
							title:bt,
							newid:count
						});
					}
				}
			})
			
			function initAboutData(item){
				
			}
			
		</script>
	</body>

</html>