package com.we.models;

/** 用户身份图片 */
public class UserIdentity extends AbstractDocument {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1910589179277698977L;

	private String realname;// 真实姓名
	private String number;// 证件号码
	private String[] pics;// 相册
	private int status;

	public String[] getPics() {
		return pics;
	}

	public void setPics(String[] pics) {
		this.pics = pics;
	}

	public int getStatus() {
		return status;
	}

	public void setStatus(int status) {
		this.status = status;
	}

	public String getRealname() {
		return realname;
	}

	public void setRealname(String realname) {
		this.realname = realname;
	}

	public String getNumber() {
		return number;
	}

	public void setNumber(String number) {
		this.number = number;
	}

	public UserIdentity() {
		super();
	}

	public UserIdentity(long _id, String[] pics, int status, String realname, String number) {
		super();
		this._id = _id;
		this.pics = pics;
		this.status = status;
		this.realname = realname;
		this.number = number;
	}

}
