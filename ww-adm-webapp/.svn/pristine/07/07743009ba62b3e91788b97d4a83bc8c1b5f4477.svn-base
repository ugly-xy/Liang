<#import "/spring.ftl" as spring />
<#import "/admin/layouts/common.xhtml" as layout>
<@layout.title>金币收支记录</@layout.title> <@layout.metaKeywordsDescription>
</@layout.metaKeywordsDescription> <@layout.cssLink> </@layout.cssLink>
<@layout.pageHeader> 金币收支记录 </@layout.pageHeader> <@layout.mainContent>
<input type="hidden" id="nav_id" value="stCoinIO" />
<div class="well well-sm">
	<div class="row">
		<FORM action="/admin/stCoinIO.xhtml">
			<div class="col-sm-3 form-group form-inline">
				<input class="form-control" id="st" name="st" type="text"
					onClick="WdatePicker()" value="${st!}" placeholder="统计日期：默认本日" />
			</div>

			<div class="col-sm-4 form-group form-inline">
			<LABEL>类型:</LABEL>
				<select class="form-control" id="type" name="type">
				<option value="">全部</option>
				<option value="0"    <#if type?? && type==0>selected</#if> >总计</option>
				<option value="1"    <#if type?? && type==1>selected</#if> >订单</option>
				<option value="2"    <#if type?? && type==2>selected</#if> >签到</option>
				<option value="3"    <#if type?? && type==3>selected</#if> >管理员</option>
				<option value="5"    <#if type?? && type==5>selected</#if> >注册前三天赠送</option>
				<option value="6"    <#if type?? && type==6>selected</#if> >使用道具</option>
				<option value="7"    <#if type?? && type==7>selected</#if> >送礼物</option>
				<option value="8"    <#if type?? && type==8>selected</#if> >改名</option>
				<option value="10"   <#if type?? && type==10>selected</#if> >机器人加币送花</option>
				<option value="11"   <#if type?? && type==11>selected</#if> >增加标签</option>
				<option value="101"  <#if type?? && type==101>selected</#if> >任务</option>
				<option value="102"  <#if type?? && type==102>selected</#if> >兑换金币</option>
				<option value="111"  <#if type?? && type==111>selected</#if> >排行榜</option>
				<option value="112"  <#if type?? && type==112>selected</#if> >词语审核</option>
				<option value="113"  <#if type?? && type==113>selected</#if> >圈子奖励</option>
				<option value="114"  <#if type?? && type==114>selected</#if> >活动</option>
				<option value="116"  <#if type?? && type==116>selected</#if> >猜猜</option>
				<option value="130"  <#if type?? && type==130>selected</#if> >谁是卧底</option>
				<option value="131"  <#if type?? && type==131>selected</#if> >你画我猜</option>
				<option value="132"  <#if type?? && type==132>selected</#if> >吹牛</option>
				<option value="133"  <#if type?? && type==133>selected</#if> >大乱斗</option>
				<option value="136"  <#if type?? && type==136>selected</#if> >狼人杀</option>
				<option value="140"  <#if type?? && type==140>selected</#if> >教室</option>
				<option value="141"  <#if type?? && type==141>selected</#if> >斗兽棋</option>
				<option value="150"  <#if type?? && type==150>selected</#if> >奴隶打工</option>
				<option value="151"  <#if type?? && type==151>selected</#if> >偷金币</option>
				<option value="152"  <#if type?? && type==152>selected</#if> >升级星球</option>
				<option value="153"  <#if type?? && type==153>selected</#if> >买奴隶</option>
				</select>
			</div>
			<div class="col-sm-3">
				<button type="submit" class="btn btn-default">查询</button>
			</div>
		</FORM>
	</div>
</div>
<#if balance?? >今日金币数目 ：${(balance)!}</#if>
<div class="row">
	<div class="col-lg-12">
		<div class="dataTable_wrapper">
			<table class="table table-striped table-bordered table-hover"
				id="dataTables-example">
				<thead>
					<tr>
						<th>日期</th>
						<th>类型</th>
						<th>用户支出</th>
						<th>用户收入</th>
					</tr>
				</thead>
				<tbody>
					<#list page.list as it>
					<tr class="gradeA">
						<td>${(it.day)!}</td>
						<td class="center">
						<#if it.type?? && it.type==0>总计
						<#elseif it.type?? && it.type==1>订单
						<#elseif it.type?? && it.type==2>签到
						<#elseif it.type?? && it.type==3>管理员
						<#elseif it.type?? && it.type==5>注册前三天赠送
						<#elseif it.type?? && it.type==6>使用道具
						<#elseif it.type?? && it.type==7>送礼物
						<#elseif it.type?? && it.type==8>改名		
						<#elseif it.type?? && it.type==10>机器人加币送花		
						<#elseif it.type?? && it.type==11>增加标签		
						<#elseif it.type?? && it.type==101>任务		
						<#elseif it.type?? && it.type==102>兑换金币		
						<#elseif it.type?? && it.type==111>排行榜	
						<#elseif it.type?? && it.type==112>词语审核		
						<#elseif it.type?? && it.type==113>圈子奖励		
						<#elseif it.type?? && it.type==114>活动		
						<#elseif it.type?? && it.type==116>猜猜		
						<#elseif it.type?? && it.type==130>谁是卧底		
						<#elseif it.type?? && it.type==131>你画我猜		
						<#elseif it.type?? && it.type==132>吹牛		
						<#elseif it.type?? && it.type==133>大乱斗		
						<#elseif it.type?? && it.type==136>狼人杀		
						<#elseif it.type?? && it.type==140>教室		
						<#elseif it.type?? && it.type==141>斗兽棋		
						<#elseif it.type?? && it.type==150>奴隶打工		
						<#elseif it.type?? && it.type==151>偷金币		
						<#elseif it.type?? && it.type==152>升级星球
						<#elseif it.type?? && it.type==153>买奴隶
						<#else></#if>(${(it.type)!})</td>
						<td>${(it.outAmount)!}</td>
						<td>${(it.inAmount)!}</td>
					</tr>
					</#list>
				</tbody>
			</table>
		</div>
		<div class="row">
			<div class="col-sm-4">
				<div class="dataTables_info" id="dataTables-example_info"
					role="status" aria-live="polite">当前
					${page.curPage}/${page.totalPage}页， 共${page.total}条</div>
			</div>
			<div class="col-sm-8"><@vpage cur=page.curPage
				total=page.totalPage url=page.url > ${vpage}</@vpage></div>
		</div>
	</div>

	<!-- /.col-lg-12 -->
</div>

</@layout.mainContent> <@layout.script> </@layout.script>