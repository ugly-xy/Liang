<#import "/spring.ftl" as spring /> <#import
"/admin/layouts/common.xhtml" as layout>
<@layout.title>push推送</@layout.title> <@layout.metaKeywordsDescription>

</@layout.metaKeywordsDescription> <@layout.cssLink>
<link rel="stylesheet" type="text/css"
	href="/webuploader/webuploader.css"></@layout.cssLink>
	<@layout.pageHeader> push推送 </@layout.pageHeader> <@layout.mainContent>
	<input type="hidden" id="nav_id" value="push" />
<div class="row">
	<#if msg??>
		结果：${(msg.code)!} 推送数据：${(msg.data)!}
	</#if>
</div>


<div class="row">
	<div class="col-lg-12">
		<form action="/admin/push" role="form" id="form" method="post">
			<div class="row">
				<div class="col-sm-3">标题</div>
				<div class="col-sm-3">
					<input class="form-control" id="title" name="title" placeholder=""
						value="${(title)!}">
				</div>
			</div>
			<div class="row">
				<div class="col-sm-3">内容</div>
				<div class="col-sm-3">
					<input class="form-control" id="body" name="body" placeholder=""
						value="${(body)!}">
				</div>
			</div>
			<div class="row">
				<div class="col-sm-3">
					<label>透传</label>
				</div>
				<div class="col-sm-3">
					<select class="form-control" id="op" name="op">
						<option value="url">url</option>
						<option value="tool">tool</option>
						<option value="eif">eif</option>
						<option value="toolTag">toolTag</option>
						<option value="topic">topic</option>
						<option value="article">article</option>
						<option value="toptic">toptic</option>
						<option value="userSpace">userSpace</option>
						<option value="chat">chat</option>
						<option value="add_friend">add_friend</option>
						<option value="third_login">third_login</option>
					</select>
				</div>
			</div>
			<div class="row">
				<div class="col-sm-3">OPid</div>
				<div class="col-sm-3">
					<input class="form-control" id="opid" name="opid" placeholder=""
						value="${(opid)!}">
				</div>
			</div>
			<div class="row">
				<div class="col-sm-3">
					<label>推送平台</label>
				</div>
				<div class="col-sm-3">
					<select  class="form-control" id="platform" name="platform">
						<option value="0">全部</option>
						<option value="2">ios全部</option>
						<option value="1">Android全部</option>
					</select>
				</div>
			</div>
			<div class="row">
				<div class="col-sm-3">
					<label>推送用户Id</label>
				</div>
				<div class="col-sm-3">
					<input class="form-control" id="id" name="id"
						placeholder="推送id" value="">
				</div>
			</div>

			<button id="submit" class="btn btn-default">推送</button>
			<button id="reset" class="btn btn-default">重置</button>

		</form>
	</div>

</div>
<!-- /.row (nested) -->

</@layout.mainContent> <@layout.script> </@layout.script>
