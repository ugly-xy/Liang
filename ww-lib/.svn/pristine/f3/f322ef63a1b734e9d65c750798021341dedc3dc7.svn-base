package com.zb.service.room.vo;

public class DiceRoomInfo extends RoomInfo {

	public DiceRoomInfo(int type) {
		super(type);
	}

	private static final long serialVersionUID = 5496615672344186605L;

	private long currentActor; // 当前操作人员
	private long totalCoinPool; // 当前奖金池

	public long getCurrentActor() {
		return currentActor;
	}

	public void setCurrentActor(long currentActor) {
		this.currentActor = currentActor;
	}

	public long getTotalCoinPool() {
		return totalCoinPool;
	}

	public void setTotalCoinPool(long totalCoinPool) {
		this.totalCoinPool = totalCoinPool;
	}

}
