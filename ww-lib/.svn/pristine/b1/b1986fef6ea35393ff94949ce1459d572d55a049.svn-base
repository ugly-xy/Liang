package com.zb.models.paper;

public class Item {
	
	private long id;
	private Long qId;
	private String content;
	private double score;//
	private int contentType;//content内容 1文本 2图片
	
	public Item(){
		
	}
	
	public Item(String content,double score,int contentType){
		this.content =content;
		this.score = score;
		this.contentType = contentType;
	}
	
	public Item(long id,Long qId,String content,double score,int contentType){
		this.id = id;
		this.qId = qId;
		this.content =content;
		this.score = score;
		this.contentType = contentType;
	}
	
	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	
	public Long getpId() {
		return qId;
	}

	public void setpId(Long qId) {
		this.qId = qId;
	}

	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	public double getScore() {
		return score;
	}
	public void setScore(double score) {
		this.score = score;
	}
	public int getContentType() {
		return contentType;
	}
	public void setContentType(int contentType) {
		this.contentType = contentType;
	}
	

}