package com.zb.service.room.vo;

import java.io.Serializable;

public class GainSlaveVO implements Serializable {

	public static final int OWN = 1;
	public static final int STEAL = 2;
	private static final long serialVersionUID = 9102274151029864512L;
	private int type;
	private int gain;
	private boolean canUp;
	private String nextWork;

	public String getNextWork() {
		return nextWork;
	}

	public void setNextWork(String nextWork) {
		this.nextWork = nextWork;
	}

	public int getType() {
		return type;
	}

	public int getGain() {
		return gain;
	}

	public boolean isCanUp() {
		return canUp;
	}

	public void setType(int type) {
		this.type = type;
	}

	public void setGain(int gain) {
		this.gain = gain;
	}

	public void setCanUp(boolean canUp) {
		this.canUp = canUp;
	}

}
