package com.zb.models;

public class Notification extends AbstractDocument {

	private static final long serialVersionUID = 87729172402553165L;
	public static boolean OPEN = true;
	public static boolean CLOSE = false;

	private boolean message = true; // 通知
	private boolean disturb = true; // 免打扰
	private int st = 23;
	private int et = 9;

	public Notification() {
		super();
	}

	public Notification(boolean message, boolean disturb, int st, int et) {
		super();
		this.message = message;
		this.disturb = disturb;
		this.st = st;
		this.et = et;
	}

	public boolean isMessage() {
		return message;
	}

	public void setMessage(boolean message) {
		this.message = message;
	}

	public boolean isDisturb() {
		return disturb;
	}

	public void setDisturb(boolean disturb) {
		this.disturb = disturb;
	}

	public int getSt() {
		return st;
	}

	public void setSt(int st) {
		this.st = st;
	}

	public int getEt() {
		return et;
	}

	public void setEt(int et) {
		this.et = et;
	}

}
