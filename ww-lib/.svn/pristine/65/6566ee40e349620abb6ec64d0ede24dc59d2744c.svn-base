package com.zb.models.room.activity;

import com.zb.models.AbstractDocument;

public class Punish extends AbstractDocument {

	/**
	 * 
	 */
	private static final long serialVersionUID = 6895731734124099392L;
	

	public Punish() {

	}

	public Punish(Long id, String txt, int status) {
		this._id = id;
		this.txt = txt;
		this.status = status;
		this.updateTime = System.currentTimeMillis();
	}

	private String txt;
	private int status;


	public int getStatus() {
		return status;
	}

	public void setStatus(int status) {
		this.status = status;
	}

	public String getTxt() {
		return txt;
	}

	public void setTxt(String txt) {
		this.txt = txt;
	}

}
