		<#include "/common/footer.xhtml"/>
		<script src="/active/assets/flexslider/jquery.flexslider-min.js"></script>
		<script src="/active/assets/js/LiftEffect.min.js" type="text/javascript" charset="utf-8"></script>
			<script type="text/javascript">
				
				$(window).load(function() {
					LiftEffect({
						"control1": ".menutips", //侧栏电梯的容器
						"control2": ".menuul", //需要遍历的电梯的父元素
						"target": [".header", ".banner", ".whatcandy", ".candywhy",".task"], //监听的内容，注意一定要从小到大输入
						"current": "active" //选中的样式
					});
					$('#imgslider').flexslider({
						animation: "fade",
						animationDuration:500,
						slideshowSpeed:3000,
						animationLoop: true,
						pauseOnHover:true,
						slideshow: false,
						directionNav:false
					});
					function editbannerbg(){
						var bgurl  = $(".flex-active-slide img").attr('src')
						$(".flex-active-slide img").hide();
						$(".flex-active-slide").css("background","url("+bgurl+") no-repeat")
					}
					var imgleng =  $(".banner .slides li");
					
					for(var i=0;i<imgleng.length;i++){
					//alert(i)
						$(".banner .slides li").eq(i).css("background","url(/active/assets/image/bnimg-0"+(i)+".jpg)")
						
					}
					
						
				});
		</script>
