						<li><img src="/active/assets/image/cointelegraph.png" height="40"/></li>
					</ul>
				</div>
			</div>
			
		</section>
		
		<div id="menutips" class="menutips">
			<ul class="menuul">
				<li class="active">1</li>
				<li>2</li>
				<li>3</li>
				<li>4</li>
				<li>5</li>
			</ul>
			<div class="img-message">
				<img src="/active/assets/image/img-message.jpg" width="69" height="80"/>
			</div>
		</div>
		<div class="iframebody">
			<iframe id="messagecon" src="/chat.xhtml" name="chat"></iframe>
			<div class="close"><img src="/active/assets/image/down-ico.png"/></div>
			<#if uid??>
			<#else>
			<div class="Maskdiv">
				<div class="box">
					<p>未登录，请您
				<a href="/login.xhtml" class="loginbtn">Login</a>!
				</p>
				</div>
				
			</div>
			</#if>
