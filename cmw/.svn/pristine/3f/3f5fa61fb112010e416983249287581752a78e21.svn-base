section {
	padding: 60px 0;
}

.container {
	width: 1200px;
	margin: 0 auto;
	overflow: hidden;
}

.header {
	height: 100px;
	background: #FFFFFF;
	/*margin-bottom: 22px;*/
	border-bottom: 1px solid #eaeaea;
}

.header .container {
	overflow: visible;
}

.header .pull-left {
	padding-top: 30px;
	display: inline-block;
}

.header .pull-left .logo {
	float: left;
}

.header .pull-right {
	float: right;
	padding-top: 30px;
}

.header nav {
	float: left;
	margin-right: 30px;
	margin-left: 94px;
}

.header nav ul li {
	display: inline-block;
	font-size: 18px;
	line-height: 40px;
	height: 40px;
	padding: 0 35px;
	position: relative;
}

.header nav ul li::before {
	content: "";
	height: 10px;
	width: 1px;
	background: #E5E5E5;
	position: absolute;
	right: 0;
	top: 14px;
}

.header nav ul li:last-child::before {
	display: none;
}

.langlist {
	display: inline-block;
	width: 90px;
	height: 30px;
	position: relative;
	overflow: visible;
	z-index: 99;
	text-align: center;
}

.langlist img {
	margin-right: 5px;
}

.langlist span {
	font-size: 14px;
}

.langlist::before {
	content: '';
	position: absolute;
	width: 6px;
	height: 6px;
	background: linear-gradient(to top, #979797, #979797) no-repeat, linear-gradient(to right, #979797, #979797) no-repeat, linear-gradient(135deg, #fff, #fff 6px, hsla(0, 0%, 100%, 0) 6px) no-repeat;
	background-size: 6px 1px, 1px 6px, 6px 6px;
	transform: rotate(-135deg);
	right: 15px;
	top: 3px;
}

.langlist:hover::before {
	transform: rotate(45deg);
	top: 7px;
}

.langlist .updiv {
	display: none;
	position: absolute;
	left: 0;
	top: 30px;
	width: 90px;
	padding: 10px 0;
	text-align: center;
	-moz-box-shadow: 1px 0px 10px 1px #d9d9d9;
	/* for firefox */
	-webkit-box-shadow: 1px 0px 10px 1px #d9d9d9;
	/* for safari or chrome */
	box-shadow: 1px 0px 10px 1px #d9d9d9;
	border-radius: 4px;
	-moz-border-radius: 4px;
	-ms-border-radius: 4px;
	-o-border-radius: 4px;
	background: #FFFFFF;
}

.langlist .updiv::after {
	position: absolute;
	left: 30px;
	top: -10px;
	content: '';
	width: 0;
	height: 0;
	border-left: 14px solid transparent;
	border-right: 14px solid transparent;
	border-bottom: 10px solid #fff;
}

.loginbtn {
	display: inline-block;
	margin-left: 30px;
	/*display: none;*/
}

.loginbtn a {
	display: inline-block;
	font-size: 16px;
	width: 90px;
	height: 26px;
	line-height: 26px;
	text-align: center;
	color: #D0021B;
	border-radius: 15px;
	-moz-border-radius: 15px;
	-ms-border-radius: 15px;
	-o-border-radius: 15px;
	border: 1px solid #D0021B;
	margin-top: 5px;
}

.loginbtn a:hover {
	color: #fff;
	background: #FF79A5;
	border-color: #FF79A5;
}

.loginbtn a:active {
	background: #E4346D;
	border-color: #E4346D;
}

.mypersonal {
	display: inline-block;
	margin-top: 10px;
}

.mypersonal a {
	text-decoration: underline;
	display: inline-block;
}

.mypersonal a.mytel {
