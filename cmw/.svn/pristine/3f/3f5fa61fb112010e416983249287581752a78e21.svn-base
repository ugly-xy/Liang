/*鐧婚檰*/

.login {
	background: url(../assets/image/login-bg-01.png) no-repeat center center;
	background-size: contain;
	height: 100vh;
	width: 100%;
	padding: 0;
	align-items: center;
	display: -webkit-flex;
}

.login .container {
	position: relative;
	overflow: visible;
	padding: 0;
}

.login .scene {}

.login .layer {
	position: absolute;
}

.login .layer1 {
	left: 0;
	top: 400px;
}

.login .layer2 {
	left: 1000px;
	top: -100px;
}

.login .layer3 {
	left: auto;
	right: 0;
	top: 300px;
}

.login .layer4 {
	left: 50%;
	top: 30%;
}

.login-content {
	position: relative;
	z-index: 99;
	width: 584px;
	margin: 0 auto;
	border-radius: 8px;
	-moz-border-radius: 8px;
	-ms-border-radius: 8px;
	-o-border-radius: 8px;
	-moz-box-shadow: 0px 2px 20px #ccc;
	/* for firefox */
	-webkit-box-shadow: 0px 2px 20px #ccc;
	/* for safari or chrome */
	box-shadow: 0px 2px 20px #ccc;
	margin-bottom: 40px;
}

.login-body {
	background: #fff;
	width: 494px;
	padding: 24px 45px;
	padding-bottom: 60px;
	border-radius: 8px;
	-moz-border-radius: 8px;
	-ms-border-radius: 8px;
	-o-border-radius: 8px;
	-moz-box-shadow: 0px 3px 1px #FF4984;
	/* for firefox */
	-webkit-box-shadow: 0px 3px 1px #FF4984;
	/* for safari or chrome */
