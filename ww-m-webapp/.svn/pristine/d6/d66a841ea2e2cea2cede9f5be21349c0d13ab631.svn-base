* {
    padding: 0;
    margin: 0;
}
.clear{
	clear: both;
}


html,
body {
    background-color: #fb4c58;
    width: 100%;
    height: 100%;
}

body {
    -webkit-touch-callout: none;
    -webkit-text-size-adjust: none;
    -webkit-appearance: none;
    -webkit-tap-highlight-color: transparent;
    -webkit-user-drag: none;
    -webkit-user-select: none;
    text-align: justify;
    text-justify: inter-ideograph;
    font-family: "Microsoft YaHei", "Hiragino Sans GB W3", "Heiti SC", Helvetica, sans-serif;
}
.header {
    position: relative;
    top: 0;
}

.header_top img {
    width: 100%;
    height: auto;
}

.header_top .font_date {
    position: relative;
   	bottom: 3.8rem;
    text-align: center;
    font-size: 1rem;
    color: #fc5556;
}


/*排行榜*/


.ranking{
	position: relative; 
	margin: 0 auto;
	width: 90%;
	margin-top: -2rem;
	/*padding-top: 1rem;*/
}

.ranking_border{
	position: relative;
	margin: 0 auto;
	width: 100%;
	padding-top: 0rem;
	border-radius: .5rem;
	height: 42rem;
	background-color: #fff0f0;
	border: .2rem solid #FD818B;
}


.num_num{
	padding: 1rem 0 0 1rem ;
	position: relative;
	left: -.5rem;
	top: .5rem;
}
.num{
	width: 1.5rem;
	position: relative;
	top: .6rem;
	padding: 0 .5rem 0 .5rem;
}
.num,.head{
	display: inline;
	float: left;
}
.head{
	width: 55%;
}

.num .num_pm{
	width: 1.5rem;
}
.head .num_head{
	display: block;
	float: left;
	width: 3rem;
}
.head .num_dj{
	padding: .5rem 0 0 .5rem;
	width: 3.4rem;
}

.head .num_gender{
	height: 1rem;
	padding:.1rem 0 0 0;
}

.head .num_vip{
	height: 1rem;
	padding:.1rem 0 0 0;
}

.num_nc{
	display: block;
	margin-top:-.2rem;
}
.num_nc_1{

	display: block;
	clear:none;
	padding: 0 0 0 3.5rem;
	
}
.num_flower{
	position: relative;
	padding: 1.3rem 1rem 0 0 ;
	display: block;
	float: right;
}
li{
	clear: both;
	padding: .8rem 0 0 0;
}


.footer_rule{
	position: relative;
	width: 100%;
	top: 2rem;
}

.footer_rule .footer_rule_img img{

	width: 26%;
	height: auto;
	display: block;
	margin: 0 auto;
}
.footer_rule_size{
	position: relative;
	top: 1rem;
	color: #ffd1d4;
	font-size: .9rem;
	width: 80%;
	margin: 0 auto;
}

	/*注意事项*/
.footer_note{
	position: relative;
	width: 100%;
	top: 6rem;
	padding-bottom: 5rem;
}

.footer_note .footer_note_img img{

	width: 26%;
	height: auto;
	display: block;
	margin: 0 auto;
}
.footer_note_size{
	position: relative;
	top: 1rem;
	color: #ffd1d4;
	font-size: .9rem;
	width: 80%;
	margin: 0 auto;
}

.footer_body ul li{
	/*padding-bottom: .4rem;*/
}