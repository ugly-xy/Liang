<!DOCTYPE html>
<html lang="zh">
<head>
	<meta charset="UTF-8">
    <title>邀您加入游戏！</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="renderer" content="webkit">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimumcale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">
    <meta name="format-detection" content="telephone=no">
    <meta name="screen-orientation" content="portrait">
    <meta name="x5-orientation" content="portrait">
    <meta name="Keywords" content="千里情缘花似海，丰富豪礼换不停！">
    <meta name="Description" content="和最有趣的人一起边玩边聊！">
    <link rel="stylesheet" href="/active/inviteFriends/css/css.css">
    <script type="text/javascript" src="//res.cdn.openinstall.io/openinstall.js"></script>
    <script type="text/javascript" src="/active/inviteFriends/js/js.js"></script>
     <script src="https://lkme.cc/js/linkedme.min.js" ></script>
    <script src="/active/js/jquery-1.11.1.min.js"></script>
</head>
<body>
		<div class="box">
			<!-- 名字／头像／游戏类型头部 -->
			<div class="box-head">
				<div class="img-box">
					<img class="head-icon l" src="${(avatar)!}" alt="">
					<div class="name l">${(nickname)!}</div>
					<img class="head-icon-qp" src="/active/inviteFriends/img/dhk-def.png" alt="">
				</div>
				<div class="l"><b class="nhwc-game l">${(gameName)!}</b></div>
				<img class="nhwc l" src="/active/inviteFriends/img/invite-def.png" alt="">
				
				<div class="nl"></div>
			</div>	
			<div class="box-inner">
				<div class="num">
					<p class="fjh">房间号</p>
					<b class="fjh-num">${(roomId)!}</b>
					   <a class="linkedme" id="go" href="#"><div class="run">进入游戏</div></a>
				</div>
			</div>
			<div class="footer">
				<img class="footer" src="/active/inviteFriends/img/def-border-bottom.png" alt="">
			</div>
		</div>
		<div class="footer-logo">
			<img class="bibi-logo" src="/active/inviteFriends/img/footer_def_logo.png" alt="">
		</div>
		
<script type="text/javascript">
linkedme.init("${(key)!}", {type: "${(type)!}"}, null);
var data = {};
data.type = "${(type)!}";  //模式
//AppStore下载地址
data.ios_custom_url = "https://itunes.apple.com/us/app/bibi%E5%A8%B1%E4%B9%90%E7%A4%BE%E5%8C%BA-%E4%B8%80%E8%B5%B7%E6%9D%A5%E7%8E%A9%E5%90%A7/id1100160696?mt=8&ign-mpt=uo%3D4"; 
//安卓平台下App的下载地址
data.android_custom_url = "http://a.app.qq.com/o/simple.jsp?pkgname=com.zhuangbi#opened";
// 自定义深度链接参数
data.params = '{"roomId":"${(roomId)!}","type":"${(gameType)!}","date":"${(date)!}"}'; 
//data.params = '{"roomId":"${(roomId)!}","type":"${(gameType)!}"}'; 
linkedme.link(data, function(err, data){
  if(err){
	  //深度链接生成失败  跳到应用宝
	  document.location = "http://a.app.qq.com/o/simple.jsp?pkgname=com.zhuangbi#opened";
  } else {
	//生成成功,设置按钮的href
    $("#go").attr("href",data.url); 
  }
},false);
</script>
</body>
</html>