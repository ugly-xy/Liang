<!DOCTYPE html>
<html>
<head>
<title>装点逼</title>

</head>
<body>
<div class="content">
	<div class="container">
		<div class="four">		
		  <h1>装点逼</h1>
		   <a  id="download" href="javascript:void(0);">下载</a>
		</div>
	</div>
</div>
<script src="/js/jquery.min.js"></script>
<script type="text/javascript">
    $(function(){
    	var sUserAgent = navigator.userAgent.toLowerCase();
    	var WeChat = sUserAgent.match(/MicroMessenger/i) == 'micromessenger';
    	var bIsMac = sUserAgent.match(/mac/i) == "mac";
    	if (bIsMac === true) {
            window.location.href = 'https://itunes.apple.com/cn/app/wei-bo-ren-sheng-mei-you-cai/id886561253?mt=8';
        }else{
        	window.location.href = 'http://a.app.qq.com/o/simple.jsp?pkgname=com.zhuangbi';
        }
        function browserRedirect() {
            if(WeChat){
                $('#WeChatTip').show().find('#close').on('touchstart',function(){
                    $('#WeChatTip').hide();
                });
            }
            if (bIsMac === true) {
                window.location.href = 'https://itunes.apple.com/cn/app/wei-bo-ren-sheng-mei-you-cai/id886561253?mt=8';
            }else{
            	window.location.href = 'http://a.app.qq.com/o/simple.jsp?pkgname=com.zhuangbi';
            }
        }
        $('#download').on('touchstart',function(){
            browserRedirect();
        });
        $('#download').on('click',function(){
            browserRedirect();
        });
    });
</script>
</body>
</html>