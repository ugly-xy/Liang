* {
	padding: 0;
	margin: 0;
}

.clear {
	clear: both;
}

html, body {
	background-color: #8fceff;
	width: 100%;
	height: 100%;
}

body {
	-webkit-touch-callout: none;
	-webkit-text-size-adjust: none;
	-webkit-appearance: none;
	-webkit-tap-highlight-color: transparent;
	-webkit-user-drag: none;
	-webkit-user-select: none;
	text-align: justify;
	text-justify: inter-ideograph;
	font-family: "Microsoft YaHei", "Hiragino Sans GB W3", "Heiti SC",
		Helvetica, sans-serif;
}
/*头部图片、日期*/
.header {
	position: relative;
	top: 0;
}

.header_top img {
	width: 100%;
	height: auto;
}

.header_top .font_date {
	position: relative;
	bottom: 3.8rem;
	text-align: center;
	font-size: 1rem;
	color: #000;
}

/*Tab标签切换*/
.ranking_btn {
	margin: 0 auto;
	width: 94%;
	height: 1.66rem;
}

.ranking_btn img {
	display: block;
	float: left;
	width: 33.3%;
}

/*排行榜*/
.ranking {
	position: relative;
	margin: 0 auto;
	width: 90%;
	padding-top: 1rem;
}

.ranking_border {
	position: relative;
	margin: 0 auto;
	width: 100%;
	padding-top: 0rem;
	border-radius: .5rem;
	height: 42rem;
	background-color: #acddff;
	border: .1rem solid #3f0600;
}

.ranking_content {
	/*background-color: #ffffff;*/
	/*opacity: .2;*/
	
}

.num_num {
	padding: 1rem 0 0 1rem;
	position: relative;
	left: -.5rem;
	top: .5rem;
}

.num {
	width: 1.5rem;
	position: relative;
	top: .6rem;
	padding: 0 .5rem 0 .5rem;
}

.num, .head {
	display: inline;
	float: left;
}

.head {
	width: 55%;
}

.num .num_pm {
	width: 1.5rem;
}

.head .num_head {
	display: block;
	float: left;
	width: 3rem;
}

.head .num_dj {
	padding: .5rem 0 0 .5rem;
	width: 3.4rem;
}

.head .num_gender {
	height: 1rem;
	padding: .1rem 0 0 0;
}

.head .num_vip {
	height: 1rem;
	padding: .1rem 0 0 0;
}

.num_nc {
	display: block;
}

.num_nc_1 {
	position: relative;
	left: .4rem;
	display: block;
	clear: none;
	padding: 0 0 0 .4rem;
}

.num_flower {
	position: relative;
	padding: 1.3rem 1rem 0 0;
	display: block;
	float: right;
}

li {
	clear: both;
	padding: .8rem 0 0 0;
}

/*活动规则*/
.footer_rule {
	position: relative;
	width: 100%;
	top: 2rem;
}

.footer_rule .footer_rule_img img {
	width: 26%;
	height: auto;
	position: relative;
	top: .3rem;
	padding-right: .4rem;
	margin: 0 auto;
	text-align: center;
}

.footer_rule_size {
	position: relative;
	top: 1rem;
	color: #3b6ba0;
	font-size: .9rem;
	width: 80%;
	margin: 0 auto;
}

/*注意事项*/
.footer_note {
	position: relative;
	width: 100%;
	top: 6rem;
	padding-bottom: 5rem;
}

.footer_note .footer_note_img img {
	width: 26%;
	height: auto;
	display: block;
	margin: 0 auto;
}

.footer_note_size {
	position: relative;
	top: 1rem;
	color: #3b6ba0;
	font-size: .9rem;
	width: 80%;
	margin: 0 auto;
}

.footer_body ul li {
	padding-bottom: .4rem;
}

.rule_img {
	display: block;
}